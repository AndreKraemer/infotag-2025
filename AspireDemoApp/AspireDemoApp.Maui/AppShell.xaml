<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AspireDemoApp.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AspireDemoApp.Maui"
    xmlns:views="clr-namespace:AspireDemoApp.Maui.Views"
    Title="AspireDemoApp.Maui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Wetter"
        ContentTemplate="{DataTemplate views:WeatherPage}"
        Route="WeatherPage" />

</Shell>
